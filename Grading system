let percentage=prompt("what is your percentage");
console.log(percentage,'percentage');
if (percentage>=90){console.log("A+",'grade');}
else if (percentage>=80){console.log("A",'grade');
} else if (percentage>=70){console.log("B",'grade');
} else if (percentage>=60){console.log("C",'grade');
} else if (percentage>=50){console.log("D",'grade');
} else if (percentage>=41){console.log("E",'grade');}
 else if (percentage<=40){console.log("F",'grade');}
